<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Tinder API testing app">
    <meta name="author" content="Timothee Moulin hello@timothee-moulin.me">
    <title>Tinder API</title>
</head>
<body>
{% set messages = app.session.getFlashBag.get('message') %}
{% if messages %}
    <section>
        <ul>
            {% for message in messages %}
                <li>
                    {{ message }}
                </li>
            {% endfor %}
        </ul>
    </section>
{% endif %}
{% block content %}
{% endblock %}
<footer>
    <hr />
    Tinder API testing app
</footer>
</body>
</html>
